<?xml version="1.0" encoding="utf-8"?>
<Patch>
<Operation Class="PatchOperationFindMod">
	<mods>
		<li>Outland - Redburn Pact</li> <!-- Neronix17.Outland.RedburnPact -->
	</mods>
	<match Class="PatchOperationSequence">
		<success>Normal</success>
		<operations>
        <!-- Remove Outland guns --> 
            <li Class="PatchOperationRemove"> <!-- Remove Outland_PactCatapult -->
				<xpath>Defs/ThingDef[defName="Outland_PactCatapult"]</xpath>
			</li>
            <li Class="PatchOperationRemove"> <!-- Remove Outland_Gun_PactCatapult -->
				<xpath>Defs/ThingDef[defName="Outland_Gun_PactCatapult"]</xpath>
			</li>	

			 <!-- Change Outland Components to MO components -->
			<li Class="PatchOperationReplace">
				<xpath>Defs/ThingDef[defName="Outland_PactCrossbow"]/costList</xpath>
					<value>
						<costList>
							<WoodLog>35</WoodLog>
							<DankPyon_ComponentBasic>1</DankPyon_ComponentBasic>
						</costList>
					</value>
			</li>






		</operations>				
	</match>
</Operation>		
</Patch>
